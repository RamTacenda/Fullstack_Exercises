package com.Assignment3;

class Shape{
	int height;
	int width;
	void getArea(int height, int width) {
		 this.height = height;
		 this.width = width;
		 this.display();
	}
	void display() {
		System.out.println("The Area: "+(height*width));
	}
}

class Rectangle extends Shape{
	void getArea(int height, int width) {
		System.out.println("The Area: "+(height*width));
	}
}
public class Q3 {
	public static void main(String args[]) {
		Shape obj = new Shape();
		obj.getArea(10, 20); 
		
	}

	
}
