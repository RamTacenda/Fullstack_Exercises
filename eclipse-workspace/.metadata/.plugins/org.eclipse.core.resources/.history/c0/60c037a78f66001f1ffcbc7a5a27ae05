package com.Practices;

class A {
	String some = "Papen";
	void display(){
		System.out.println("Hey! "+ some);
	}
	
	void setter(String txt) {
		this.some = txt;
	}
	
}

public class Practice1 {
	public static void main(String args[]) {
		A obj = new A();
		obj.display();
		obj.setter("Richard");
	}
}
