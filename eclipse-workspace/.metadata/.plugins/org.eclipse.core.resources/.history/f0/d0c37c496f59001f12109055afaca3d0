package com.Assignment3;

class Vehicle{
	static void drive() {
		 System.out.println("This is a vehicle class");
	}
}

class Car extends Vehicle{
	static void drive() {
		System.out.println("Repairing a car");
	}
}
public class Q2 {
	public static void main(String args[]) {
		Car obj = new Car();
		obj.drive();
	}

}
