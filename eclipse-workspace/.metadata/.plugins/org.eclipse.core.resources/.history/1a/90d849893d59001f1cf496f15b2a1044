package com.Inheritances;

class A1{
	A1(){
		System.out.println("A");
	}
}

class B1 extends A1{
	B1(){
		System.out.println("B");
	}
	B1(int num){
		System.out.println("B 2");
	}
}

class C1 extends B1{
	C1(){
		super(24);
		System.out.println("C");
	}
}

public class SuperMultiple {

	public static void main(String args[]) {
		C1 obj = new C1();
	}
}
